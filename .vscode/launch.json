	"version": "0.2.0",
	"configurations": [

		
		{
			"type": "factoriomod",
			"request": "launch",
			"name": "Factorio Mod Debug",
			"factorioPath": "C:/Program Files/factorio/bin/x64/factorio.exe",
			"factorioArgs": ["--instrument-mod debugadapter_1.1.11"],
			"profileSlowStart": 1,
			"useInstrumentMode": true
		},
		{
			"type": "factoriomod",
			"request": "launch",
			"name": "Factorio Mod Debug (Profile)",
			"factorioPath": "C:/Program Files/factorio/bin/x64/factorio.exe",
			"hookMode": "profile",
			"factorioArgs": ["--instrument-mod debugadapter_1.1.11"],
			"profileSlowStart": 1,
			"useInstrumentMode": true
		},

		{
			"type": "lua",
			"request": "launch",
			"name": "Debug",
			"program": "${workspaceFolder}/addons/market.lua"
		}
	]
}